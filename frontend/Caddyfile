{
    admin off
    auto_https off
    log {
        output stdout
        format json {
            time_key time
            time_format iso8601
        }
    }
}

:9085

root * /var/www/html

@auth {
    header Cookie *auth=true*
}

handle @auth {
   try_files {path} users/index.html
   file_server
}
handle {
   try_files {path} index.html
   file_server
}
