{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,ts,svelte,css"
    }
  },
  "scripts": {
    "hmr": "vite --host",
    "dev": "concurrently --kill-others \"node server\" \"npx npm-watch\"",
    "build": "MODE=dev vite build",
    "prod": "MODE=prod vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prettify": "prettier --write --plugin-search-dir=. .",
    "prettify:check": "prettier --check --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@sveltejs/vite-plugin-svelte": "^2.0.3",
    "@tsconfig/svelte": "^3.0.0",
    "concurrently": "^7.6.0",
    "express": "^4.18.2",
    "npm-watch": "^0.11.0",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "svelte": "^3.57.0",
    "svelte-check": "^3.1.4",
    "svelte-fa": "^3.0.3",
    "svelte-preprocess": "^5.0.3",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-ssr": "^0.4.97"
  },
  "dependencies": {
    "@ethersproject/providers": "^5.7.2",
    "@metamask/detect-provider": "^2.0.0",
    "@stripe/stripe-js": "^1.49.0",
    "chart.js": "^4.2.1",
    "ethers": "^5.7.2",
    "humanize-duration": "^3.28.0",
    "ky": "^0.33.0",
    "svelte-chartjs": "^3.1.2",
    "svelte-qr": "^1.0.0",
    "svelte-routing": "^1.6.0",
    "svelte-simple-modal": "^1.5.2",
    "yup": "^0.32.11"
  }
}
