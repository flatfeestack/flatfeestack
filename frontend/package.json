{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,ts,svelte,css"
    }
  },
  "scripts": {
    "dev": "concurrently --kill-others \"node server\" \"npx npm-watch\"",
    "build": "MODE=dev vite build",
    "prod": "MODE=prod vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prettify": "prettier --write --plugin-search-dir=. .",
    "prettify:check": "prettier --check --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@sveltejs/vite-plugin-svelte": "^1.1.0",
    "@tsconfig/svelte": "^3.0.0",
    "concurrently": "^7.5.0",
    "express": "^4.18.2",
    "npm-watch": "^0.11.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.8.0",
    "svelte": "^3.52.0",
    "svelte-check": "^2.9.2",
    "svelte-fa": "^3.0.3",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-ssr": "^0.4.44"
  },
  "dependencies": {
    "@ethersproject/providers": "^5.7.2",
    "@metamask/detect-provider": "^2.0.0",
    "@stripe/stripe-js": "^1.42.1",
    "chart.js": "^4.0.0-alpha.3",
    "ethers": "^5.7.2",
    "ky": "^0.32.2",
    "svelte-chartjs": "^3.0.1",
    "svelte-qr": "^1.0.0",
    "svelte-routing": "^1.6.0",
    "svelte-simple-modal": "^1.4.1"
  }
}